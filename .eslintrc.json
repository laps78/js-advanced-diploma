{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
      "max-len": "off",
      "no-unused-vars": "warn",
      "no-alert": "off",
      "arrow-parens": "warn",
      "class-methods-use-this": "warn",
      "no-restricted-syntax": "warn"
    }
}
